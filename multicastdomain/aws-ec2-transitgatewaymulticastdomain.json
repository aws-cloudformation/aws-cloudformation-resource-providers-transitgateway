{
    "typeName": "AWS::EC2::TransitGatewayMulticastDomain",
    "description": "The AWS::EC2::TransitGatewayMulticastDomain type segments the multicast network into different domains and makes the transit gateway act as multiple multicast routers",
    "sourceUrl": "https://github.com/aws-cloudformation/aws-cloudformation-resource-providers-transitgateway.git",
    "definitions": {
        "Tag": {
            "description": "A key-value pair to associate with a transit gateway multicast domain.",
            "type": "object",
            "properties": {
                "key": {
                    "type": "string"
                },
                "value": {
                    "type": "string"
                }
            }
        }
    },
    "properties": {
        "TransitGatewayId": {
            "description": "The ID of the transit gateway",
            "type": "string"
        },
        "TransitGatewayMulticastDomainId": {
            "description": "The ID of the transit gateway multicast domain.",
            "type": "string"
        },
        "TagSet": {
            "description": "The tags for the transit gateway multicast domain.",
            "type": "array",
            "items": {
                "$ref": "#/definitions/Tag"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "TransitGatewayId"
    ],
    "createOnlyProperties": [
        "/properties/TransitGatewayId"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "EC2:CreateTransitGatewayMulticastDomain"
            ]
        },
        "read": {
            "permissions": [
                "EC2:DescribeTransitGatewayMulticastDomains"
            ]
        },
        "delete": {
            "permissions": [
                "EC2:DeleteTransitGatewayMulticastDomain"
            ]
        },
        "list": {
            "permissions": [
                "EC2:DescribeTransitGatewayMulticastDomains"
            ]
        }
    }
}
